    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "env-path-key": "^1.0.19",
    "path-string-prepend": "^1.0.24",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
